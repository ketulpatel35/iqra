<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>

        <!-- ============== Sequences for account voucher ==================== -->
        <record id="seq_type_registration" model="ir.sequence.type">
            <field name="name">Account Voucher From Payfort Resend</field>
            <field name="code">voucher.payfort</field>
        </record>

        <record id="seq_registration" model="ir.sequence">
            <field name="name">Account Voucher From Payfort Resend</field>
            <field name="code">voucher.payfort</field>
            <field name="prefix">PYFORT</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- ============== Sequences order id for next acd year ==================== -->
        <record id="seq_next_year_advance_fee" model="ir.sequence.type">
            <field name="name">Next Year Advance Fee</field>
            <field name="code">next.year.adv.fee</field>
        </record>

        <record id="next_year_advance_fee_sequence" model="ir.sequence">
            <field name="name">Next Year Advance Fee</field>
            <field name="code">next.year.adv.fee</field>
            <field name="prefix">NYAF</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</openerp>